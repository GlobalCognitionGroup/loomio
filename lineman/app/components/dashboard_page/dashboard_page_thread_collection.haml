%section.dashboard-page-threads-container{ng-show: 'any()', role: 'region', aria-label: "{{ name | translate }}"}
  .dashboard-page-date-range{translate: 'dashboard_page.threads_from.{{name}}', aria-hidden: 'true'}
  .dashboard-page-threads
    %dashboard_page_thread{discussion: 'thread', ng-repeat: 'thread in filteredThreads()'}
